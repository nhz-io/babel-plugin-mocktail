{
  "name": "babel-plugin-mocktail",
  "version": "0.0.4",
  "description": "Mocktail.mock() your sources",
  "repository": {
    "type": "git",
    "url": "https://github.com/nhz-io/babel-plugin-mocktail.git"
  },
  "author": "Ishi Ruy <dev@nhz.io>",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "devDependencies": {
    "babel-cli": "*",
    "babel-core": "^6.5.2",
    "babel-eslint": "*",
    "babel-preset-es2015": "*",
    "babel-preset-stage-0": "*",
    "chai": "*",
    "coveralls": "*",
    "esdoc": "*",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "*",
    "ghooks": "*",
    "isparta": "*",
    "mocha": "*",
    "mocktail": "*",
    "normalize-newline": "*",
    "sinon-chai": "*"
  },
  "scripts": {
    "clean": "rm -rf lib doc coverage",
    "lint": "eslint src test",
    "prebuild": "npm run clean -s",
    "build": "babel src -d lib",
    "test": "mocha --recursive --require babel-core/register ./test/unit",
    "travis": "npm run-script clean && npm run-script build && npm run-script coveralls",
    "test:watch": "npm run test -- --watch",
    "prepublish": "npm run build -s",
    "coverage": "babel-node ./node_modules/isparta/bin/isparta cover node_modules/mocha/bin/_mocha  -- --reporter dot --recursive test/unit",
    "coveralls": "npm run-script coverage -s && node node_modules/coveralls/bin/coveralls < ./coverage/lcov.info || echo Coverage Failed",
    "postcoveralls": "rm -rf ./coverage",
    "postpublish": "git push origin master --follow-tags",
    "deploy": "git pull --rebase origin master && git push origin master",
    "toc": "doctoc --github --title \"# Changelog\" CHANGELOG.md",
    "doc": "node ./node_modules/.bin/esdoc -c esdoc.json"
  },
  "config": {
    "ghooks": {
      "commit-msg": "m=$(cat $2);git rev-parse --abbrev-ref HEAD | sed -n -r 's/^(issue-([0-9]+)|.+)$/\\2/;s/^[0-9]+$/#&/p' | tr '\n' ' ' > $2 && echo $m >> $2"
    }
  },
  "bugs": {
    "url": "https://github.com/nhz-io/sass2styl/issues"
  },
  "homepage": "https://nhz-io.github.io/babel-plugin-mocktail",
  "keywords": [
    "babel",
    "plugin",
    "mocktail",
    "babel-plugin",
    "babel-plugin-mocktail"
  ],
  "license": "MIT"
}

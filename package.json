{
  "name": "babel-plugin-mocktail",
  "version": "0.0.2",
  "description": "mocktail babel plugin",
  "repository": "nhz-io/babel-plugin-mocktail",
  "author": "Ishi Ruy <dev@nhz.io>",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "coveralls": "~2.11.6",
    "esdoc": "^0.4.5",
    "isparta": "~4.0.0",
    "mocha": "^2.2.5",
    "mocktail": "^0.3.8",
    "normalize-newline": "^2.0.0"
  },
  "scripts": {
    "clean": "rm -rf lib doc coverage",
    "lint": "eslint src test",
    "check": "npm run lint -s && dependency-check package.json --entry src",
    "prebuild": "npm run check -s && npm run clean -s",
    "build": "babel src -d lib",
    "test": "mocha --recursive --require babel-core/register",
    "travis": "npm run-script clean && npm run-script build && npm run-script coveralls",
    "test:watch": "npm run test -- --watch",
    "prepublish": "npm run clean && npm run build",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover node_modules/mocha/bin/_mocha -- --recursive --reporter dot",
    "coveralls": "npm run-script coverage -s && coveralls < coverage/lcov.info",
    "postcoveralls": "rm -rf ./coverage",
    "prepublish": "npm run build -s",
    "postpublish": "git push origin master --follow-tags",
    "deploy": "git pull --rebase origin master && git push origin master",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "toc": "doctoc --github --title \"# Changelog\" CHANGELOG.md"
  },
  "config": {
    "ghooks": {
      "commit-msg": "m=$(cat $2);git rev-parse --abbrev-ref HEAD | sed -n -r 's/^(issue-([0-9]+)|.+)$/\\2/;s/^[0-9]+$/#&/p' | tr '\n' ' ' > $2 && echo $m >> $2"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhz-io/babel-plugin-mocktail.git"
  },
  "bugs": {
    "url": "https://github.com/nhz-io/sass2styl/issues"
  },
  "homepage": "https://nhz-io.github.io/babel-plugin-mocktail",
  "keywords": [
    "babel",
    "plugin",
    "mocktail",
    "babel-plugin",
    "babel-plugin-mocktail"
  ],
  "license": "MIT"
}

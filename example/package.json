{
  "name": "babel-plugin-mocktail-example",
  "private": true,
  "version": "1.0.0",
  "description": "babel-plugin-mocktail example usage",
  "main": "src/main.js",
  "scripts": {
    "test": "npm run-script test:original && npm run-script test:mocked",
    "test:original": "babel-node --plugins mocktail test/original.test.js | tspec",
    "test:mocked": "babel-node --plugins mocktail test/mocked.test.js | tspec"
  },
  "author": "Ishi Ruy <dev@nhz.io>",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-mocktail": "*",
    "mocktail": "^0.3.8",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0"
  }
}

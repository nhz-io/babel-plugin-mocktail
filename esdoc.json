{
  "source": "./src",
  "destination": "./doc",
  "includes": ["\\.(js|es6)$"],
  "excludes": ["\\.config\\.(js|es6)$"],
  "unexportIdentifier": false,
  "undocumentIdentifier": true,
  "builtinExternal": true,
  "index": "./README.md",
  "package": "./package.json",
  "coverage": true,
  "includeSource": true,
  "test": {
    "type": "mocha",
    "source": "./test",
    "includes": ["\\.(js|es6)$"],
    "excludes": ["\\.config\\.(js|es6)$"]
  },
  "lint": true,
  "plugins": [
    {"name": "esdoc-es7-plugin"}
  ]
}
